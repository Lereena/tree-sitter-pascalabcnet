==================
cycles and condition
==================

begin
  var a, b, c, d: int64;
  Readln(a, b, c, d);
  for var x := -100 to 100 do
  begin
    var k := 0;
    if d + x * (c + x * (b + a * x)) = 0 then
    begin
      k += 1;
      Print(x);
      if k = 3 then exit
    end
  end
end.

