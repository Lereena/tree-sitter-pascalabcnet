==================
lambda
==================

begin
  var w := ReadArrInteger(3);
  var c := ReadArrInteger(3);
  var r := ReadArrInteger(3);
  var res := ArrGen(3, i -> c[i]*r[i] - w[i]);
  var max := res.Max;
  if max <= 0 then
    Println(0)
  else
    for var i := 0 to 2 do
      if res[i] = max then
        Print(i+1);
end.

