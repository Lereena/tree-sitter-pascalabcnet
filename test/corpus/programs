==================
function
==================

function Arithm(n: integer; a0,h: real): sequence of real;
begin
  var x := a0;
  loop n do
  begin
    yield x;
    x += h;
  end;
end;

begin
  Arithm(10,11,3).Println;
  foreach var x in Arithm(10,11,3) do
    Print(x);
end.

