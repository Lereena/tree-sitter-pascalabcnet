==================
slice
==================

begin
  var s := '';
  foreach var w in ReadlnString.MatchValues('\d*[A-Z]') do
  begin
    var l := w.Length;
    s += l = 1 ? w : w[:l].ToInteger * w[l]
  end;
  s.Batch(40, c -> c.JoinIntoString).PrintLines
end.

